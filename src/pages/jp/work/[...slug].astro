---
import { getCollection } from "astro:content";

import type { AstroSeoProps } from "@astrolib/seo";

import MarkdownPostLayout from "../../../layouts/MarkdownPostLayout.astro";

export async function getStaticPaths() {
	const projectsEntries = await getCollection("projects");

	return projectsEntries
		.map((entry) => {
			const [lang, ...slug] = entry.slug.split("/");
			return { params: { lang, slug: slug.join("/") || undefined }, props: entry };
		})
		.filter((entry) => entry.params.lang === "jp");
}

const entry = Astro.props;
const { Content } = await entry.render();

const seo: AstroSeoProps = {
	title: "Authors - Sreehari R",
	description:
		"Machine Learning | DeepLearning | CSE (AI & ML) Undergrad",	
	canonical: `https://sree14hari.work/jp/work/${entry.slug}/`,
	
};
---

<MarkdownPostLayout frontmatter={entry.data} body={entry.body} seo={seo}>
	<Content />
</MarkdownPostLayout>
